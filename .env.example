# Environment Configuration
NODE_ENV=production

# Port Configuration
# INTERNAL_PORT: Port inside the container
# EXTERNAL_PORT: Port exposed on the host machine
INTERNAL_PORT=XXXX
EXTERNAL_PORT=XXXX

# Development Port Configuration (for docker-compose.dev.yml)
INTERNAL_PORT_DEV=XXXX
EXTERNAL_PORT_DEV=XXXX

# Security - Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
